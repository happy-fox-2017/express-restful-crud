<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>create</title>
  </head>
  <body>
    <h1>Create</h1>
    <form class="" action="/memos/create" method="post">
      <label for="">todo:</label><input type="text" name="todo" value="">
      <select class="" name="user">
        <option value="" disabled selected>Enter User</option>
        <% users.forEach(user => {%>
        <option value="<%= user.id %>"><%= user.name %></option>
        <% }) %>
      </select>
      <input type="submit" name="" value="Submit">
    </form>
    <a href="/">
      <button type="button" name="button">Back</button>
    </a>
  </body>
</html>